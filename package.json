{
  "name": "instacart-live-challenge",
  "scripts": {
    "apply-migrations": "docker compose run dbmate",
    "build": "rm -rf ./build && tsc",
    "new-migration": "docker compose run dbmate new --",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet && prettier --check '{src,test}/**/*.ts' && prettier --check '**/*.{yml,yaml}'",
    "linter-fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && npm run prettier",
    "prettier": "./node_modules/prettier/bin/prettier.cjs --write .",
    "seed": "npx ts-node db/seed/index.ts",
    "source": "export $(cat .env | egrep -v '#|^$' | xargs)",
    "start": "npx ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.2.0",
    "@types/passport": "^1.0.16",
    "@types/pg-format": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "axios": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "format": "^0.2.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "pg": "^8.12.0",
    "pg-format": "^1.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/pg": "^8.11.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0"
  }
}
